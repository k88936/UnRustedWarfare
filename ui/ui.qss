QLineEdit {
	border-width: 1px; 
	border-radius: 0px;
	border-color: #505050;
	border-style: inset;
	padding: 0px;
	color: #a9b7c6;
	background:#151515;
	selection-background-color:#505050;
	selection-color: #2BDABD;
}
QLabel {
	color: #a9b7c6;
	background-color: transparent;
}
/* QLCDNumber {
	color: #37e6b4;
} */ 
QProgressBar {
	color: transparent;
	border-style:inset;
	border-width: 1px; 
	border-radius: 0px;
	border-color: #30F6D9;
	background-color:#151515;
}
QProgressBar::chunk {
	background-color: #30F6D9;
	width:5px;
	margin:0px;
	border-radius: 0px;
}
