QStatusBar {
	color:#ffffff;
}
QSpinBox {
	color: #a9b7c6;	
	background-color: #000000;
}
QDoubleSpinBox {
	color: #a9b7c6;	
	background-color: #000000;
}
QTimeEdit {
	color: #a9b7c6;	
	background-color: #000000;
}
QDateTimeEdit {
	color: #a9b7c6;	
	background-color: #000000;
}
QDateEdit {
	color: #a9b7c6;	
	background-color: #000000;
}
QComboBox {
	color: #a9b7c6;	
	background: #000000;
}
QComboBox:editable {
	background: #000000;
	color: #a9b7c6;
	selection-background-color: #000000;
}
QComboBox QAbstractItemView {
	color: #a9b7c6;	
	background: #000000;
	selection-color: #FFFFFF;
	selection-background-color: #000000;
}
QComboBox:!editable:on, QComboBox::drop-down:editable:on {
	color: #a9b7c6;	
	background: #000000;
}
QFontComboBox {
	color: #a9b7c6;	
	background-color: #000000;
}
QToolBox {
	color: #a9b7c6;
	background-color: #000000;
}
QToolBox::tab {
	color: #a9b7c6;
	background-color: #000000;
}
QToolBox::tab:selected {
	color: #FFFFFF;
	background-color: #000000;
}
QScrollArea {
	color: #a9b7c6;
	background-color: #000000;
}
QSlider{
background-color: #000000
}
QSlider::groove:horizontal {
	height: 1px;
}
QSlider::groove:vertical {
	width: 1px;
}
QSlider::handle{
	background-color: #32FFE0;
	border: 1px solid #151515;
	border-radius: 0px;
}
QSlider::handle:horizontal {
	width: 6px;
	margin: -4px 0;
}
QSlider::handle:vertical {
	height: 6px;
	margin: 0 -4px;
}
QSlider::add-page{
    background: #a9b7c6;
}
QSlider::sub-page {
    background: #2BDABD;
}
