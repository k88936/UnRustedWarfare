QTabWidget {
	color:#a9b7c6;
	background-color:#000000;
	
}
QTabWidget::pane {
	border:1px solid #151515;
	border-radius: 0px;
}
QTabBar::tab{
	border:1px solid #000000;
	border-radius: 0px;
	color: #a9b7c6;
	padding:8px;
	background-color: #000000;
}
QTabBar::tab:top,QTabBar::tab:bottom{
min-width:80px;
}
QTabBar::tab:left,QTabBar::tab:right{
min-height:80px;
}
 QTabBar::tab:hover {
	color: #30F6D9;
}
QTabBar::tab:top:hover{
	border-bottom-color:#30F6D9;
}
QTabBar::tab:bottom:hover{
	border-top-color:#30F6D9;
}
QTabBar::tab:left:hover{
	border-right-color:#30F6D9;
}
QTabBar::tab:right:hover{
	border-left-color:#30F6D9;
}
QTabBar::tab:selected, QTabBar::tab:last:selected {
	color: #000000;
	border-color: #32FFE0;
	background-color: #30F6D9;
}

QCheckBox {
	color: #a9b7c6;
	border:1px solid #151515;
	border-radius:0px;
	background-color:#151515;
	padding-left: 4px;
}


QCheckBox:hover {
	border-color: #30F6D9;
	color:#30F6D9;
}
QCheckBox::indicator{
	height: 10px;
	width: 10px;
	border-style:solid;
	border-width: 1px;
	border-color: #a9b7c6;
}
QCheckBox::indicator:checked {
	background-color: #30F6D9;
	border-color:#30F6D9
}
QCheckBox::indicator:unchecked {
	background-color: transparent;
}
QCheckBox:disabled {
	color: #505050;
}
