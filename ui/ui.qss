QMainWindow {
	background-color:#000000;
}
QWidget{
 	background-color: #000000;
}
QDialog {
	background-color:#000000;
}
QColorDialog {
	background-color:#000000;
}
QTextEdit {
	background-color:#000000;
	color: #a9b7c6;
} 
QPlainTextEdit {
	selection-background-color:#007b50;
	background-color:#000000;
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-width: 1px;
	color: #a9b7c6;
} 
QPushButton{
	border-style: solid;
	border-radius:0px;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #30F6D9;
	border-bottom-width: 1px;
	border-style: solid;
	color: #a9b7c6;
	padding:5px;
	background-color: #151515;
	
}

QPushButton::default{
	border-bottom-width: 2px;
	border-bottom-color: #32FFE0;
	color: #32FFE0;
}
QPushButton:flat{
	color:#000000;
	background-color:#30F6D9;
}
QPushButton:hover{
	color: #000000;
	background-color: #30F6D9;
	
}
QPushButton:pressed{
	color: #000000;
	border-bottom-color: #2BDABD;
	background-color: #2BDABD;
}
QPushButton:disabled{
	border-bottom-color: #505050;
	color: #505050;
	background-color: #151515;
}
