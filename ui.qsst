QLineEdit {
	border-width: 1px; 
	border-radius: 0px;
	border-color: $color_dark3;
	border-style: inset;
	padding: 0px;
	color: $color_gray;
	background:$color_dark2;
	selection-background-color:$color_dark3;
	selection-color: $color_high_light;
}
QLabel {
	color: $color_gray;
	background-color: transparent;
}
/* QLCDNumber {
	color: #37e6b4;
} */ 
QProgressBar {
	color: transparent;
	border-style:inset;
	border-width: 1px; 
	border-radius: 0px;
	border-color: $color_high_light2;
	background-color:$color_dark2;
}
QProgressBar::chunk {
	background-color: $color_high_light2;
	width:5px;
	margin:0px;
	border-radius: 0px;
}
