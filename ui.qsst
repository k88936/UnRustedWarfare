QTabWidget {
	color:$color_gray;
	background-color:$color_dark;
	
}
QTabWidget::pane {
	border:1px solid $color_dark2;
	border-radius: 0px;
}
QTabBar::tab{
	border:1px solid $color_dark;
	border-radius: 0px;
	color: $color_gray;
	padding:8px;
	background-color: $color_dark;
}
QTabBar::tab:top,QTabBar::tab:bottom{
min-width:80px;
}
QTabBar::tab:left,QTabBar::tab:right{
min-height:80px;
}
 QTabBar::tab:hover {
	color: $color_high_light2;
}
QTabBar::tab:top:hover{
	border-bottom-color:$color_high_light2;
}
QTabBar::tab:bottom:hover{
	border-top-color:$color_high_light2;
}
QTabBar::tab:left:hover{
	border-right-color:$color_high_light2;
}
QTabBar::tab:right:hover{
	border-left-color:$color_high_light2;
}
QTabBar::tab:selected, QTabBar::tab:last:selected {
	color: $color_dark;
	border-color: $color_high_light3;
	background-color: $color_high_light2;
}

QCheckBox {
	color: $color_gray;
	border:1px solid $color_dark2;
	border-radius:0px;
	background-color:$color_dark2;
	padding-left: 4px;
}


QCheckBox:hover {
	border-color: $color_high_light2;
	color:$color_high_light2;
}
QCheckBox::indicator{
	height: 10px;
	width: 10px;
	border-style:solid;
	border-width: 1px;
	border-color: $color_gray;
}
QCheckBox::indicator:checked {
	background-color: $color_high_light2;
	border-color:$color_high_light2
}
QCheckBox::indicator:unchecked {
	background-color: transparent;
}
QCheckBox:disabled {
	color: $color_dark3;
}
