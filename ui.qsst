$color_high_light = #2BDABD;
$color_high_light2 = #30F6D9;
$color_high_light3 = #32FFE0;

$color_dark = #000000;
$color_dark2 = #151515;
$color_dark3 = #505050;

$color_gray = #a9b7c6;
$color_bright = #FFFFFF;
QMainWindow {
	background-color:$color_dark;
}
QWidget{
 	background-color: $color_dark;
}
QDialog {
	background-color:$color_dark;
}
QColorDialog {
	background-color:$color_dark;
}
QTextEdit {
	background-color:$color_dark;
	color: $color_gray;
} 
QPlainTextEdit {
	selection-background-color:#007b50;
	background-color:$color_dark;
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-width: 1px;
	color: $color_gray;
} 
QPushButton{
	border-style: solid;
	border-radius:0px;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: $color_high_light2;
	border-bottom-width: 1px;
	border-style: solid;
	color: $color_gray;
	padding:5px;
	background-color: $color_dark2;
	
}

QPushButton::default{
	border-bottom-width: 2px;
	border-bottom-color: $color_high_light3;
	color: $color_high_light3;
}
QPushButton:flat{
	color:$color_dark;
	background-color:$color_high_light2;
}
QPushButton:hover{
	color: $color_dark;
	background-color: $color_high_light2;
	
}
QPushButton:pressed{
	color: $color_dark;
	border-bottom-color: $color_high_light;
	background-color: $color_high_light;
}
QPushButton:disabled{
	border-bottom-color: $color_dark3;
	color: $color_dark3;
	background-color: $color_dark2;
}
