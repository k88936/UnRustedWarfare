QStatusBar {
	color:#ffffff;
}
QSpinBox {
	color: $color_gray;	
	background-color: $color_dark;
}
QDoubleSpinBox {
	color: $color_gray;	
	background-color: $color_dark;
}
QTimeEdit {
	color: $color_gray;	
	background-color: $color_dark;
}
QDateTimeEdit {
	color: $color_gray;	
	background-color: $color_dark;
}
QDateEdit {
	color: $color_gray;	
	background-color: $color_dark;
}
QComboBox {
	color: $color_gray;	
	background: $color_dark;
}
QComboBox:editable {
	background: $color_dark;
	color: $color_gray;
	selection-background-color: $color_dark;
}
QComboBox QAbstractItemView {
	color: $color_gray;	
	background: $color_dark;
	selection-color: #FFFFFF;
	selection-background-color: $color_dark;
}
QComboBox:!editable:on, QComboBox::drop-down:editable:on {
	color: $color_gray;	
	background: $color_dark;
}
QFontComboBox {
	color: $color_gray;	
	background-color: $color_dark;
}
QToolBox {
	color: $color_gray;
	background-color: $color_dark;
}
QToolBox::tab {
	color: $color_gray;
	background-color: $color_dark;
}
QToolBox::tab:selected {
	color: #FFFFFF;
	background-color: $color_dark;
}
QScrollArea {
	color: $color_gray;
	background-color: $color_dark;
}
QSlider{
background-color: $color_dark
}
QSlider::groove:horizontal {
	height: 1px;
}
QSlider::groove:vertical {
	width: 1px;
}
QSlider::handle{
	background-color: $color_high_light3;
	border: 1px solid $color_dark2;
	border-radius: 0px;
}
QSlider::handle:horizontal {
	width: 6px;
	margin: -4px 0;
}
QSlider::handle:vertical {
	height: 6px;
	margin: 0 -4px;
}
QSlider::add-page{
    background: $color_gray;
}
QSlider::sub-page {
    background: $color_high_light;
}
