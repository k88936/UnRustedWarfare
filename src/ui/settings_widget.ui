	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-width: 1px;
	color: #B5B5B5;
} 
QPushButton{
	border:0px solid transparent;
	border-radius:0px;
	border-bottom-color: #30F6D9;
	border-bottom-width: 1px;
	color: #B5B5B5;
	padding:5px;
	background-color: #50151515;	
}
QPushButton::default{
	border-bottom-width: 2px;
	border-bottom-color: #32FFE0;
	color: #32FFE0;
}
QPushButton:flat{
	color:#ff000000;
	background-color:#30F6D9;
}
QPushButton:hover{
	color: #ff000000;
	background-color: #30F6D9;
	
}
QPushButton:pressed,QPushButton::checked{
	color: #ff000000;
	border-bottom-color: #26C3A9;
	background-color: #26C3A9;
}
QPushButton:disabled{
	border-bottom-color: #50505050;
	color: #505050;
	background-color: #50151515;
}
